{
  "applicationId": "dune.server.interface.alpha",
  "version": "0.1.0",
  "defaultMode": "window",
  "port": 3333,
  "documentRoot": "/build/interface",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "globalVariables": {

  },
  "modes": {
    "window": {
      "title": "duneServerAlpha",
      "width": 1000,
      "height": 900,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/src/interface/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/interface/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
        "filesystem.*",
        "os.*"
      ]
    }
  },
  "cli": {
    "binaryName": "duneServerInterface",
    "resourcesPath": "/build/interface/",
    "extensionsPath": "/src/extensions/",
    "clientLibrary": "/src/interface/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}