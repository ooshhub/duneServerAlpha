{
	"files": [
		"./src/server/main.ts",
	],
	"include": [
		"./src/server/**/*",
		"./src/types/**/*"
	],
	"compilerOptions": {
		"typeRoots": [ "/src/types" ],
		"rootDir": "./src/server",
		"outDir": "build/server",
		"target": "ES2019",
		"module": "CommonJS",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"types": ["node"],
		"strict": true,
	},
}
